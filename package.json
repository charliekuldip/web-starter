{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bs-reload": "browser-sync reload",
    "build": "npm run build:css && npm run build:js",
    "build:css": "npm run sass",
    "build:js": "webpack",
    "dev-server": "webpack --progress --colors --watch",
    "postsass": "postcss -u autoprefixer --autoprefixer.browsers \"> 4%, ie 10, last 5 versions\" -r dist/css/*.min.css",
    "sass": "node-sass --source-map true --output-style compressed src/scss/main.scss dist/css/main.min.css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:dev-server": "webpack && npm run dev-server",
    "watch:css": "nodemon -e scss --watch src/scss -x npm run build:css",
    "watch:css-compilation": "nodemon -e css --watch dist/css -x npm run bs-reload",
    "watch:html": "nodemon -e html --watch dist/ -x npm run bs-reload"
  },
  "author": "Charles Davis",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "bell-on-bundler-error-plugin": "^1.0.8",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "flexbox-grid-mixins": "^0.1.3",
    "node-sass": "^3.10.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "flexboxgrid": "^6.3.1",
    "imagesloaded": "^4.1.1",
    "jquery": "^3.1.1",
    "normalize.css": "^4.2.0",
    "onscreen": "^1.2.0",
    "postcss-cli": "^2.6.0",
    "promise": "^7.1.1",
    "qs": "^6.2.1",
    "selectric": "^1.11.0",
    "slick-carousel": "^1.6.0"
  }
}
